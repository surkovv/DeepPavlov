{
  "dataset_reader": {
    "class_name": "sq_reader",
    "valid_size": 1000,
    "data_path": "{DOWNLOADS_PATH}/ms-marco-t5-small.json"
  },
  "dataset_iterator": {
    "class_name": "data_learning_iterator"
  },
  "chainer": {
    "in": ["graph"],
    "pipe": [
      {
        "class_name": "deeppavlov.models.graph2text.triplets_preprocessor:TripletsPreprocessor",
        "in": ["graph"],
        "out": ["preprocessed_graph"]
      },
      {
        "class_name": "deeppavlov.models.graph2text.triplets_tokenizer:TripletsTokenizer",
        "tokenizer_path": "/data/home/admin/qa/DeepPavlov/deeppavlov/models/graph2text/JointGT/pretrain_model/jointgt_bart",
        "in": ["preprocessed_graph"],
        "out": ["input_ids"]
      },
      {
        "class_name": "deeppavlov.models.graph2text.graph2text_bart:GraphToTextBart",
        "checkpoint_path": "/data/home/admin/qa/DeepPavlov/deeppavlov/models/graph2text/JointGT/checkpoint/jointgt_bart_webnlg",
        "tokenizer_path": "/data/home/admin/qa/DeepPavlov/deeppavlov/models/graph2text/JointGT/pretrain_model/jointgt_bart",
        "in": ["input_ids"],
        "out": ["answer"]
      }
    ],
    "out": ["answer"]
  },
  "metadata": {
    "variables": {
      "PROJECT_PATH": "/data/home/admin/qa",
      "ROOT_PATH": "~/.deeppavlov",
      "DOWNLOADS_PATH": "{ROOT_PATH}/downloads",
      "MODELS_PATH": "{ROOT_PATH}/models",
      "MODEL_PATH": "{MODELS_PATH}/generative_qa/first_train/ms-marco--max-tok-"
    },
    "download": [
      {
        "url": "https://files.deeppavlov.ai/deeppavlov_data/generative_question_answering_new/ms_marco_small.pth.tar",
        "subdir": "{MODEL_PATH}"
      }
    ]
  }
}
